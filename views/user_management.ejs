<%- include('layout', { title: 'Dashboard - Budget Claims System' }) %>
    <main>
        <h2>Existing Users</h2>
        <ul>
            <% users.forEach(user => { %>
                <li>
                    <p>Name: <%= user.name %></p>
                    <p>Email: <%= user.email %></p>
                    <p>Role: <%= user.role %></p>
                </li>
            <% }) %>
        </ul>
    </main>