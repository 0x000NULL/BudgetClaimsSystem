<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>User Management</title>
    <link rel="stylesheet" href="/css/styles.css">
</head>
<body>
    <header>
        <h1>User Management</h1>
    </header>
    <main>
        <h2>Existing Users</h2>
        <ul>
            <% users.forEach(user => { %>
                <li>
                    <p>Name: <%= user.name %></p>
                    <p>Email: <%= user.email %></p>
                    <p>Role: <%= user.role %></p>
                </li>
            <% }) %>
        </ul>
    </main>
</body>
</html>
