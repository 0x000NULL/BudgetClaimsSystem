{
  "name": "BudgetClaimsSystem", // Name of the project
  "version": "1.4.27", // Version of the project
  "description": "A system to manage vehicle claims for Budget Las Vegas.", // Brief description of the project
  "main": "server.js", // Entry point of the application
  "scripts": {
    "start": "node server.js", // Script to start the application
    "dev": "nodemon server.js", // Script to start the application in development mode with nodemon
    "test": "jest --coverage", // Script to run tests with Jest and generate coverage reports
    "test:watch": "jest --watchAll", // Script to run tests in watch mode with Jest
    "cypress": "cypress open" // Script to open Cypress for end-to-end testing
  },
  "dependencies": {
    "archiver": "^7.0.1", // Module for creating archives (zip, tar, etc.)
    "bcryptjs": "^2.4.3", // Library to hash passwords
    "body-parser": "^1.19.0", // Middleware to parse incoming request bodies
    "cache-manager": "^3.6.3", // Cache management library
    "cache-manager-redis-store": "^2.0.0", // Redis store for cache-manager
    "connect-flash": "^0.1.1", // Flash message middleware for Express
    "connect-mongo": "^5.1.0", // MongoDB session store for Express
    "cors": "^2.8.5", // Middleware to enable CORS
    "csv-express": "^1.2.2", // Middleware to convert JSON to CSV
    "dotenv": "^16.4.5", // Module to load environment variables from a .env file
    "ejs": "^3.1.10", // Embedded JavaScript templating
    "exceljs": "^4.4.0", // Excel file creation library
    "express": "^4.19.2", // Web framework for Node.js
    "express-fileupload": "^1.5.0", // Middleware for handling file uploads
    "express-session": "^1.17.1", // Session middleware for Express
    "file-upload": "^0.0.0", // Module for file upload (unused, as express-fileupload is also included)
    "helmet": "^4.6.0", // Security middleware for Express
    "jsonwebtoken": "^8.5.1", // Library for JSON Web Token authentication
    "method-override": "^3.0.0", // Middleware to support HTTP verbs such as PUT or DELETE
    "mongoose": "^5.10.3", // MongoDB object modeling tool
    "morgan": "^1.10.0", // HTTP request logger middleware
    "multer": "^1.4.5-lts.1", // Middleware for handling multipart/form-data
    "node-cron": "^3.0.0", // Task scheduler for Node.js
    "nodemailer": "^6.4.11", // Module to send emails
    "passport": "^0.4.1", // Authentication middleware
    "passport-local": "^1.0.0", // Local authentication strategy for Passport
    "path": "^0.12.7", // Utility for handling and transforming file paths
    "pdfkit": "^0.12.3", // Library to create PDF documents
    "pdfmake": "^0.2.10", // Library to generate PDF documents in the browser
    "qrcode": "^1.4.4", // Library to generate QR codes
    "redis": "^3.1.2", // Redis client library
    "roboto-font": "^0.1.0", // Roboto font package
    "speakeasy": "^2.0.0", // Two-factor authentication library
    "xlsx": "^0.16.9" // Library to parse and write Excel files
  },
  "devDependencies": {
    "@testing-library/react": "^16.0.0", // React testing library
    "cypress": "^13.13.0", // End-to-end testing framework
    "dotenv-cli": "^6.0.0", // Command line tool to load environment variables from .env files
    "eslint": "^8.0.0", // JavaScript linter
    "eslint-plugin-jest": "^27.0.0", // ESLint plugin for Jest
    "jest": "^29.7.0", // JavaScript testing framework
    "mongodb-memory-server": "^9.4.0", // In-memory MongoDB server for testing
    "nodemon": "^2.0.4", // Tool to automatically restart the application when file changes are detected
    "supertest": "^7.0.0" // Library for testing HTTP endpoints
  },
  "author": "Ethan Aldrich", // Author of the project
  "license": "All Rights Reserved" // License type
}
